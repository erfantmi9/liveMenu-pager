{
  "expo": {
    "name": "livemenu-pager",
    "slug": "livemenu-pager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#787878"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-128x128.png",
        "backgroundColor": "#787878"
      },
      "package": "com.erfantmi.livemenupager"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "2855dd35-72a5-433f-909d-b352fcfb2996"
      }
    }
  }
}
